    "@openzeppelin/contracts": "^5.4.0",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "dotenv": "^16.1.4",
    "web3": "^1.10.0",
    "ethers": "^5.7.2",
    "hardhat": "^3.1.0",
    "solc": "^0.8.17"
  }
